<div class="jumbotron center">
	<h1>Welcome to the app.</h1>

	<% if user_signed_in? %>
		<p>signed in</p>
	<% else %>
		<p>
			<%= link_to "login", new_user_session_path, class: "btn btn-default btn-lg" %> 
			<%= link_to "sign up here", new_user_registration_path, class: "btn btn-primary btn-lg" %>
		</p>
	<% end %>

</div>